{
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "diagnostic.hintSign": "•",
  "git.addedSign.text": "+",
  "git.addedSign.hlGroup": "DiffAdd",
  "git.removedSign.text": "-",
  "git.removedSign.hlGroup": "DiffDelete",
  "git.changedSign.text": "~",
  "git.changedSign.hlGroup": "DiffChange",
  "java.referencesCodeLens.enabled": true,
  "java.configuration.runtimes": [
    {
        "name": "JavaSE-1.8",
        "path": "/lib/jvm/java-8-openjdk-amd64"
      },
    {
        "name": "JavaSE-11",
        "path": "/lib/jvm/java-11-openjdk-amd64"
      }
  ],
  "languageserver": {
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "markdownlint.config": {
    "default": true,
    "MD013": false
  },
  "coc-actions.showActionKind": true,
  "coc-actions.useCursorLine": true,
  "coc-actions.hideCursor": false,
  "cSpell.ignorePaths": [
    "**/package-lock.son",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/site-packages/**",
    "**/.git/objects/**",
    ".vscode",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "cSpell.ignoreRegExpList": [
    "#\\S+",
    "@\\S+",
    "Plug \\'\\S+",
    "(?<=@)([A-Z]\\w+)(\\s[A-Z]\\w+)*",
    "(?<=\\[.*\\])\\(.*\\)"
  ],
  "cSpellExt.enableDictionaries": [
    "catalan",
    "medical-terms"
  ],
  "cSpell.language": "en,ca",
  "cSpell.showStatus": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.userWords": [
    "CELERYD",
    "Devlife",
    "GGrep",
    "Goyo",
    "Jira",
    "Kibana",
    "Kinesalite",
    "NuGet",
    "POSTGRES",
    "Rebased",
    "Shuhel",
    "Techlead",
    "Testim",
    "Youtube",
    "ahmedshuhel",
    "ansible",
    "asyncio",
    "autoload",
    "autossh",
    "datepicker",
    "defx",
    "devenv",
    "dircolors",
    "dotfiles",
    "dpage",
    "echom",
    "errno",
    "etcd",
    "execvp",
    "func",
    "gbrowse",
    "getenv",
    "gruvbox",
    "guids",
    "hackathon",
    "httpx",
    "isnot",
    "itertools",
    "kclpy",
    "kubectl",
    "kubernetes",
    "lazyload",
    "mccabe",
    "mygroup",
    "mypy",
    "nargs",
    "ncdocker",
    "neovim",
    "noqa",
    "nvim",
    "obj",
    "omnisharp",
    "pgadmin",
    "pgcli",
    "prevchunk",
    "psql",
    "pycache",
