{
  "languageserver": {
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "coc-actions.showActionKind": true,
  "coc-actions.useCursorLine": true,
  "coc-actions.hideCursor": false,
  "codeLens.enable": true,
  "omnisharp.useDotnet": false,
  "python.jediEnabled": false,
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.ignoreRegExpList": [
    "#\\S+"
  ],
  "cSpell.showStatus": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.userWords": [
    "Kinesalite",
    "NuGet",
    "Techlead",
    "Testim",
    "Youtube",
    "ansible",
    "autoload",
    "autossh",
    "datepicker",
    "devenv",
    "dircolors",
    "dotfiles",
    "lazyload",
    "neovim",
    "nvim",
    "omnisharp",
    "pyenv",
    "pynvim",
    "sidenav",
    "symlink",
    "tidyrc",
    "whiteframe"
  ]
}
