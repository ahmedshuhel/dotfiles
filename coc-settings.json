{
  "codeLens.enable": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "git.addedSign.text": "+",
  "git.addedSign.hlGroup": "DiffAdd",
  "git.removedSign.text": "-",
  "git.removedSign.hlGroup": "DiffDelete",
  "git.changedSign.text": "~",
  "git.changedSign.hlGroup": "DiffChange",
  "languageserver": {
    "metals": {
      "command": "metals-vim",
      "rootPatterns": ["build.sbt"],
      "filetypes": ["scala", "sbt"]
    },
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "markdownlint.config": {
    "default": true,
    "MD013": false
  },
  "coc-actions.showActionKind": true,
  "coc-actions.useCursorLine": true,
  "coc-actions.hideCursor": false,
  "omnisharp.useDotnet": false,
  "python.jediEnabled": false,
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode"
  ],
  "cSpell.ignoreRegExpList": [
    "#\\S+",
    "Plug \\'\\S+"
  ],
  "cSpell.showStatus": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.userWords": [
    "Devlife",
    "GGrep",
    "Goyo",
    "Kinesalite",
    "NuGet",
    "Rebased",
    "Shuhel",
    "Techlead",
    "Testim",
    "Youtube",
    "ansible",
    "autoload",
    "autossh",
    "datepicker",
    "defx",
    "devenv",
    "dircolors",
    "dotfiles",
    "echom",
    "func",
    "gruvbox",
    "kclpy",
    "lazyload",
    "mygroup",
    "nargs",
    "neovim",
    "nvim",
    "obj",
    "omnisharp",
    "prevchunk",
    "pycache",
    "pyenv",
    "pynvim",
    "sidenav",
    "symlink",
    "tidyrc",
    "uname",
    "whiteframe"
  ]
}
